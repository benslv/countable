{
  "name": "countable",
  "version": "0.2.0",
  "description": "A small, robust bot to help support and manage a counting channel in your Discord server!",
  "author": "Ben Silverman <bensilverman10@gmail.com>",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier ./src/**/*.ts --write",
    "prebuild": "yarn run format && yarn run lint",
    "build": "tsc"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "license": "MIT",
  "dependencies": {
    "better-sqlite-pool": "^0.3.0",
    "discord.js": "^12.5.3",
    "emoji-regex": "^9.2.2",
    "enmap": "^5.8.5"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
