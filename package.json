{
  "name": "countable",
  "version": "0.2.0",
  "description": "A small, robust bot to help support and manage a counting channel in your Discord server!",
  "author": "Ben Silverman <bensilverman10@gmail.com>",
  "scripts": {
    "start": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier ./src/**/*.ts --write",
    "prepare": "husky install",
    "prebuild": "yarn run format && yarn run lint",
    "build": "tsc",
    "pre-commit": "lint-staged && pretty-quick --staged"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "better-sqlite-pool": "^0.3.0",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.2.0",
    "emoji-regex": "^10.0.0",
    "enmap": "^5.8.5"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
